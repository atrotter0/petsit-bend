<div class="content">
  <h1 align="center">Reservations</h1>
  <div class="data-container">
    <div class="data-header">
      <ul>
        <li>Owner</li>
        <li>Start Date</li>
        <li>Options</li>
      </ul>
    </div>

    <div class="data-content">
      <% @reservations.each do |reservation| %>
        <ul>
          <li><%= reservation.user.username %></li>
          <li><%= reservation.start_date.strftime('%a - %b %d, %Y') %></li>
          <li>View | Edit | Delete</li>
        </ul>
      <% end %>
    </div>
  </div>
</div>
