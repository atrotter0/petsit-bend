<div class="content">
  <h1 align="center">Users</h1>
  <div class="data-container">
    <div class="data-header">
      <ul>
        <li>Username</li>
        <li>Email</li>
        <li>Options</li>
      </ul>
    </div>

    <div class="data-content">
      <% @users.each do |user| %>
        <ul>
          <li><%= user.username %></li>
          <li><%= user.email %></li>
          <li>View | Edit | Delete</li>
        </ul>
      <% end %>
    </div>
  </div>
</div>
